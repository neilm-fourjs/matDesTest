	ACTION dyntext ( TEXT="Dynamic Text", IMAGE="square" )
	ACTION darklogo ( TEXT="Dark Logo", IMAGE="square" )
	ACTION lightlogo ( TEXT="Light Logo", IMAGE="square" )

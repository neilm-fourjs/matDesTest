ATTRIBUTES

GROUP g1 : g1, TEXT=%"A Group #1", GRIDCHILDRENINPARENT,STYLE="uncollapsed curved";
GROUP g2 : g2, TEXT=%"A Group #2",STYLE="collapsed";

LABEL lab1 : lab1, TEXT=%"ButtonEdit:",JUSTIFY=RIGHT;
LABEL lab2 : lab2, TEXT=%"DateEdit:",JUSTIFY=RIGHT;
LABEL de1 : de1, TEXT=%"normal", STYLE="small";
LABEL de2 : de2, TEXT=%"dropdown", STYLE="small";
LABEL lab3 : lab3, TEXT=%"DateEdit:",JUSTIFY=RIGHT;
LABEL lab2a : lab2a, TEXT=%"Interval:",JUSTIFY=RIGHT;
LABEL lab4 : lab4, TEXT=%"Combo:",JUSTIFY=RIGHT;
LABEL lab5 : lab5, TEXT=%"Combo(QE):",JUSTIFY=RIGHT;
LABEL lab6 : lab6, TEXT=%"Completer:",JUSTIFY=RIGHT;
LABEL lab7 : lab7, TEXT=%"Readonly:",JUSTIFY=RIGHT;
LABEL lab8 : lab8, TEXT=%"Live Field:",JUSTIFY=RIGHT;
LABEL lab9 : lab9, TEXT=%"Dead Field:",JUSTIFY=RIGHT;
LABEL l4stfile : l4stfile, TEXT=%"Style File:";
LABEL lfglpro : lfglpro, TEXT=%"FGLPROFILE:";
LABEL lrt : lrt, TEXT=%"Runtime:";
LABEL lcli : lcli, TEXT=%"Client:";

BUTTONEDIT f001 = FORMONLY.fld1, ACTION=win, TABINDEX=2;
DATEEDIT f002 = FORMONLY.fld2, TABINDEX=3;
DATEEDIT f003 = FORMONLY.fld3, TABINDEX=4, STYLE="dropdown";
f02a = FORMONLY.fld2a, SCROLL, TABINDEX=5;
COMBOBOX f004 = FORMONLY.fld4, INITIALIZER=cb_colour, PLACEHOLDER=%"Pick a Colour", TABINDEX=6;
COMBOBOX f005 = FORMONLY.fld5, INITIALIZER=cb_colour, PLACEHOLDER=%"Pick a Colour", QUERYEDITABLE, TABINDEX=7;
f006 = FORMONLY.fld6,COMPLETER, TABINDEX=8;

WEBCOMPONENT col = FORMONLY.col_hex,
	STYLE="curvedborder",
	SIZEPOLICY=FIXED, 
	STRETCH=NONE, 
	SCROLLBARS=NONE, 
	HEIGHT=32 PIXELS, WIDTH=64 PIXELS,
	COMPONENTTYPE="colour";

WEBCOMPONENT cl = FORMONLY.cl, COMPONENTTYPE="clock";
WEBCOMPONENT dcl = FORMONLY.dcl, COMPONENTTYPE="dclock";

f007 = FORMONLY.fld7, NOENTRY;
f008 = FORMONLY.fld8, TABINDEX=9;
f009 = FORMONLY.fld9, NOENTRY;
p101 = FORMONLY.fld10, TABINDEX=10;
p102 = FORMONLY.fld11, NOENTRY;
CHECKBOX a = FORMONLY.okay, VALUECHECKED=1, VALUEUNCHECKED=0, TEXT=%"Ok?", NOT NULL, TABINDEX=11;
CHECKBOX b = FORMONLY.notokay, VALUECHECKED=1, VALUEUNCHECKED=0, TEXT=%"NOk?", NOT NULL, TABINDEX=12;
CHECKBOX c = FORMONLY.nul, VALUECHECKED=1, VALUEUNCHECKED=0, TEXT=%"Nul?";

-- normal table
--TABLE tab : tab, WANTFIXEDPAGESIZE; -- ideally want this for @small only!
a001 = FORMONLY.col1, TITLE="Fld1", IMAGECOLUMN=img;
a002 = FORMONLY.col2, TITLE="Fld2";
a03 = FORMONLY.col3, TITLE="Fld3";
PHANTOM FORMONLY.img;

BUTTON b1 : bomb, IMAGE="heart", TEXT="", TABINDEX=1;
BUTTON but1 : quit;
BUTTON msg : msg;
BUTTON err: err;

TEXTEDIT ip = FORMONLY.imgpath,STRETCH=X;
EDIT fglprof = FORMONLY.fglprof;
TEXTEDIT fglpro = FORMONLY.fglpro,STRETCH=X, STYLE="small";
TEXTEDIT sf = FORMONLY.stylefile,STRETCH=X;
EDIT rtver = FORMONLY.rtver;
EDIT client = FORMONLY.client;


TOPMENU
	GROUP file (TEXT="File")
		COMMAND showform
		COMMAND inactive
		SEPARATOR
		COMMAND quit
	END -- FIle
	GROUP edit (TEXT="Edit")
		COMMAND editcut
		COMMAND editcopy
		COMMAND editpaste
	END -- FIle
	GROUP opt (TEXT="Option")
		COMMAND constrct
		COMMAND msg
		COMMAND err
		COMMAND win
		COMMAND win_mess
		COMMAND win_quest
		COMMAND notify
		COMMAND wintitle
		COMMAND uitext
		COMMAND dyntext
		COMMAND darklogo
		COMMAND lightlogo
		COMMAND pg
		COMMAND pg50
		COMMAND fc_ismob
		COMMAND sleep
		COMMAND clipset
		COMMAND clipshow
	END -- opt
	GROUP help (TEXT="Help")
		COMMAND about
	END -- opt
END -- TM

